<div class="main students-edit">
  <div class="container">
    <h3>ログイン</h3>
    <%= form_with url:"/users/login" do %>

      <div class="form-group">
        <label>アカウント名<input type="text" name="account" class="form-control"></label>
      </div>
      <div class="form-group">
        <label>パスワード<input type="password" name="password" class="form-control"></label>
      </div>
      <div class="form-group">
        <input type="submit" value="ログイン" class="btn btn-primary">
      </div>

    <% end %>
  </div>
</div>